<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserDetail</servlet-name>
        <servlet-class>Controller.Authentication.UserDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.Authentication.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewMemberInClass</servlet-name>
        <servlet-class>Controller.Authentication.viewMemberInClass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewSchedule</servlet-name>
        <servlet-class>Controller.Teacher.ViewSchedule</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateUserDetail</servlet-name>
        <servlet-class>Controller.Authentication.UpdateUserDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertTimetable</servlet-name>
        <servlet-class>Controller.Teacher.InsertTimetable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewTimetable</servlet-name>
        <servlet-class>Controller.Teacher.viewTimetable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomepageStudent</servlet-name>
        <servlet-class>HomePage.HomepageStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomepageTeacher</servlet-name>
        <servlet-class>HomePage.HomepageTeacher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewScheduleStudent</servlet-name>
        <servlet-class>Controller.Student.ViewScheduleStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Controller.Authentication.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserDetail</servlet-name>
        <url-pattern>/UserDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewMemberInClass</servlet-name>
        <url-pattern>/viewMemberInClass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewSchedule</servlet-name>
        <url-pattern>/ViewSchedule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateUserDetail</servlet-name>
        <url-pattern>/UpdateUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertTimetable</servlet-name>
        <url-pattern>/InsertTimetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewTimetable</servlet-name>
        <url-pattern>/viewTimetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomepageStudent</servlet-name>
        <url-pattern>/HomepageStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomepageTeacher</servlet-name>
        <url-pattern>/HomepageTeacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewScheduleStudent</servlet-name>
        <url-pattern>/ViewScheduleStudent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
